{
    "name": "@tractorzoom/serverless-mysql-utils",
    "version": "2.1.56",
    "description": "Wrapper around serverless-mysql implementing standardized error handling and automatic retries",
    "main": "lib/index.js",
    "author": "@TractorZoom",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "commit": "git-cz",
        "test": "jest",
        "prettier:all": "prettier --write './**/*.*'",
        "pretty-quick": "pretty-quick"
    },
    "devDependencies": {
        "@babel/cli": "7.12.10",
        "@babel/core": "7.12.10",
        "@babel/preset-env": "7.12.11",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "chance": "1.1.7",
        "commitizen": "4.2.2",
        "husky": "4.3.6",
        "jest": "26.6.3",
        "prettier": "2.2.1",
        "pretty-quick": "2.0.2",
        "semantic-release": "17.3.0"
    },
    "dependencies": {
        "serverless-mysql": "1.5.4"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/serverless-mysql-utils.git"
    },
    "homepage": "https://github.com/TractorZoom/serverless-mysql-utils",
    "bugs": {
        "url": "https://github.com/TractorZoom/serverless-mysql-utils/issues"
    },
    "keywords": [
        "serverless",
        "mysql",
        "serverless-mysql",
        "sql"
    ]
}
